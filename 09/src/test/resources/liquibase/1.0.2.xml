<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.0.xsd
        http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd">
        
    <property name="autoIncrement" value="false" dbms="postgresql,mssql,oracle"/>
    <property name="autoIncrement" value="true" dbms="h2"/>

        <changeSet id="1.0.2.01" author="agolubin" context="update" runInTransaction="false">
                <insert tableName="Author">
                        <column name="ID" valueComputed="1"/>
                        <column name="name" value="Лев"/>
                        <column name="sur_name" value="Толстой"/>
                </insert>
        </changeSet>
    <changeSet id="1.0.2.02" author="agolubin" context="update" runInTransaction="false">
        <insert tableName="Genre">
            <column name="ID" valueComputed="1"/>
            <column name="name" value="Роман"/>
        </insert>
    </changeSet>
    <changeSet id="1.0.2.03" author="agolubin" context="update" runInTransaction="false">
        <insert tableName="Book">
            <column name="ID" valueComputed="1"/>
            <column name="author_ID" value="1"/>
            <column name="genre_ID" value="1"/>
            <column name="name" value="Война и мир"/>
        </insert>
    </changeSet>


</databaseChangeLog>